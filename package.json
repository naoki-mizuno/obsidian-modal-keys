	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"globals": "^16.5.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.9.3"
	}
}
