{
	"version": "1.2.0",
	"name": "obsidian-modal-keys-remapper",
	"description": "Customize keyboard shortcuts for navigating modal dialogs and suggestion lists.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"modal",
		"keybindings",
		"shortcuts",
		"navigation"
	],
